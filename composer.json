    "keywords": [
        "pubsub",
        "redis",
        "php",
        "pubsub php"
    ],
    "license": "MIT",
    "authors": [{
        "name": "Rio Rizky Rainey Ferbiansyah",
        "email": "rizkyrainey@gmail.com",
